wojewodztwa <- c("Nieznane województwo", "Mazowieckie", "Małopolskie", "Łódzkie", "Dolnośląskie", 
                 "Wielkopolskie", "Pomorskie", "Zachodniopomorskie", "Kujawsko-Pomorskie", 
                 "Lubelskie", "Śląskie", "Świętokrzyskie", "Warmińsko-Mazurskie", 
                 "Lubuskie", "Opolskie", "Podkarpackie")

# Wektor miesięcy
miesiace <- c("Styczeń", "Luty", "Marzec", "Kwiecień", "Maj", "Czerwiec", 
              "Lipiec", "Sierpień", "Wrzesień", "Październik", "Listopad", "Grudzień")

# Przypisanie wartości do dataframe
df <- df %>%
  mutate(
    Region = wojewodztwa[Region + 1],  # Dodajemy 1, ponieważ indeksy w R zaczynają się od 1
    Month = miesiace[Month + 1]       # Podobnie, dodajemy 1, aby indeksy pasowały do miesięcy
  )
