result <- tryCatch({
  # Twoja funkcja, która może wygenerować ostrzeżenie lub błąd
  Mack.S.E(CLL[["Models"]], FullTriangle, est.sigma = est.sigma)
}, warning = function(w) {
  # Funkcja ta jest wywoływana, jeśli pojawi się ostrzeżenie
  message("Ostrzeżenie: ", conditionMessage(w))
  return(NULL)  # Możesz zwrócić NULL lub kontynuować obliczenia
}, error = function(e) {
  # Funkcja ta jest wywoływana, jeśli pojawi się błąd
  message("Błąd: ", conditionMessage(e))
  return(NULL)  # Możesz zwrócić NULL lub kontynuować obliczenia
})

# Jeśli wynik jest NULL, kontynuujemy pętlę
if (!is.null(result)) {
  # Twoje dalsze obliczenia, jeśli nie wystąpiły błędy
}
