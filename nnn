
Start-Process -NoNewWindow -FilePath "python" -ArgumentList "-m http.server 8080"


New-NetFirewallRule -DisplayName "Allow Python HTTP Server" -Direction Inbound -Action Allow -Protocol TCP -LocalPort 8080

Get-NetFirewallRule | Select-String "Python HTTP Server"
Get-Process | Where-Object {$_.ProcessName -like "python*"}
python -m http.server 8080
