mean_spread_function<-function(ring,exposure){
  
  mean_spread_function<-exposure/(1+exp(-mu_spread_parameters[1])*ring^(-mu_spread_parameters[2])*exposure^(-mu_spread_parameters[3]))
  
}

# - number of fire spreads var
var_spread_function<-function(ring,exposure){
  
  var_spread_function<-mean_spread_function(ring,exposure)*(1-mean_spread_function(ring,exposure)/exposure)*(1+(exposure-1)/(1+exp(-sigma_spread_parameters[1])*ring^(-sigma_spread_parameters[2])*exposure^(-sigma_spread_parameters[3])))
  
}
